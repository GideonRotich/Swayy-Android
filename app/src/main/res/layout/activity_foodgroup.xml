<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".groups.foodgroupActivity">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="54dp"
        android:id="@+id/mbappe"
        android:background="#000000">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/yes"
            >



            <ImageView
                android:layout_width="40dp"
                android:id="@+id/opt"
                android:layout_height="22dp"
                android:layout_alignParentLeft="true"
                android:src="@drawable/ic_baseline_arrow_back_24"
                android:layout_marginLeft="16dp"
                android:layout_marginTop="14dp"
                android:tint="#ffffff"
                android:backgroundTint="#ffffff"
                >

            </ImageView>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Agriculture and food"
                android:layout_toRightOf="@id/opt"
                android:layout_marginLeft="20dp"
                android:textStyle="bold"
                android:layout_marginTop="12dp"
                android:textColor="#ffffff"
                android:textSize="18sp">

            </TextView>
        </RelativeLayout>


    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/mbappe"
        android:background="#ffffff"
        android:id="@+id/leeds"
        >

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:id="@+id/jan"
            android:layout_marginTop="10dp">

            <ImageView
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_marginLeft="10dp"
                android:id="@+id/messi"
                android:src="@drawable/home_theaters">

            </ImageView>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:text="Feeds and Seeds"
                android:layout_toRightOf="@id/messi"
                android:id="@+id/ronaldo"
                android:textColor="#000000"
                android:layout_marginTop="16dp">

            </TextView>

            <ImageView
                android:layout_width="24dp"
                android:layout_height="30dp"
                android:layout_marginRight="20dp"
                android:tint="#000000"
                android:layout_marginTop="10dp"
                android:layout_alignParentRight="true"
                android:id="@+id/messibarca"

                android:src="@drawable/ic_baseline_keyboard_arrow_right_24">

            </ImageView>

        </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:id="@+id/feb"
            android:layout_below="@id/jan"
            android:layout_marginTop="20dp">

            <ImageView
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_marginLeft="10dp"
                android:id="@+id/pepe"
                android:src="@drawable/home_theaters">

            </ImageView>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:text="Livestock and Poultry"
                android:layout_toRightOf="@id/pepe"
                android:id="@+id/neymar"
                android:textColor="#000000"
                android:layout_marginTop="6dp">

            </TextView>

            <ImageView
                android:layout_width="24dp"
                android:layout_height="30dp"
                android:layout_marginRight="20dp"
                android:tint="#000000"
                android:layout_alignParentRight="true"
                android:id="@+id/neymarbarca"

                android:src="@drawable/ic_baseline_keyboard_arrow_right_24">

            </ImageView>

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:id="@+id/march"
            android:layout_below="@id/feb"
            android:layout_marginTop="20dp">

            <ImageView
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_marginLeft="10dp"
                android:id="@+id/auba"
                android:src="@drawable/home_theaters">

            </ImageView>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:text="Food and Drinks"
                android:layout_toRightOf="@id/auba"
                android:id="@+id/smith"
                android:textColor="#000000"
                android:layout_marginTop="6dp">

            </TextView>

            <ImageView
                android:layout_width="24dp"
                android:layout_height="30dp"
                android:layout_marginRight="20dp"
                android:tint="#000000"
                android:layout_alignParentRight="true"
                android:id="@+id/neymarbarca"

                android:src="@drawable/ic_baseline_keyboard_arrow_right_24">

            </ImageView>

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:id="@+id/april"
            android:layout_below="@id/march"
            android:layout_marginTop="20dp">

            <ImageView
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_marginLeft="10dp"
                android:id="@+id/carasco"
                android:src="@drawable/home_theaters">

            </ImageView>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:text="Farm Equipments"
                android:layout_toRightOf="@id/carasco"
                android:id="@+id/dimaria"
                android:textColor="#000000"
                android:layout_marginTop="6dp">

            </TextView>

            <ImageView
                android:layout_width="24dp"
                android:layout_height="30dp"
                android:layout_marginRight="20dp"
                android:tint="#000000"
                android:layout_alignParentRight="true"
                android:id="@+id/neymarpsg"

                android:src="@drawable/ic_baseline_keyboard_arrow_right_24">

            </ImageView>

        </RelativeLayout>


    </RelativeLayout>


</RelativeLayout>