<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".groups.simuFragment">


    <com.google.android.material.appbar.AppBarLayout
        android:layout_width="match_parent"
        android:layout_height="54dp"
        android:id="@+id/mbappe"
        android:elevation="2dp"
        android:background="#ffffff">

        <androidx.appcompat.widget.Toolbar
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/yes"
            >
            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">



                <ImageView
                    android:layout_width="40dp"
                    android:id="@+id/opt"
                    android:layout_height="22dp"
                    android:layout_alignParentLeft="true"
                    android:src="@drawable/ic_baseline_arrow_back_24"
                    android:layout_marginLeft="6dp"
                    android:layout_marginTop="14dp"
                    android:tint="#000000"
                    android:layout_centerVertical="true"
                    android:backgroundTint="#000000"
                    >

                </ImageView>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Mobile phones and tablets"
                    android:layout_toRightOf="@id/opt"
                    android:layout_marginLeft="14dp"
                    android:fontFamily="@font/uber_move_bold"
                    android:layout_centerVertical="true"
                    android:layout_marginTop="12dp"
                    android:textColor="@color/giddy"
                    android:textSize="18sp">

                </TextView>
            </RelativeLayout>
        </androidx.appcompat.widget.Toolbar>


    </com.google.android.material.appbar.AppBarLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/mbappe"
        android:background="#ffffff"
        android:id="@+id/leeds"
        >

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:id="@+id/jan"
            android:layout_marginTop="10dp">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:text="Accessories for mobile phones and tablets"
android:fontFamily="@font/uber_move"
                android:id="@+id/ronaldod"
                android:textColor="#000000"
                android:layout_marginTop="16dp">

            </TextView>

            <ImageView
                android:layout_width="24dp"
                android:layout_height="30dp"
                android:layout_marginRight="20dp"
                android:tint="#000000"
                android:layout_marginTop="10dp"
                android:layout_alignParentRight="true"
                android:id="@+id/messibarca"

                android:src="@drawable/ic_baseline_keyboard_arrow_right_24">

            </ImageView>

        </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:id="@+id/feb"
            android:layout_below="@id/jan"
            android:layout_marginTop="20dp">


            <TextView
                android:id="@+id/neymard"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="6dp"
                android:layout_toRightOf="@id/pepe"
                android:text="Mobile phones"
                android:fontFamily="@font/uber_move"
                android:textColor="#000000">

            </TextView>

            <ImageView
                android:layout_width="24dp"
                android:layout_height="30dp"
                android:layout_marginRight="20dp"
                android:tint="#000000"
                android:layout_alignParentRight="true"
                android:id="@+id/neymarbarca"

                android:src="@drawable/ic_baseline_keyboard_arrow_right_24">

            </ImageView>

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:id="@+id/march"
            android:layout_below="@id/feb"
            android:layout_marginTop="20dp">


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:text="Smart watches and trackers"
                android:layout_toRightOf="@id/auba"
                android:id="@+id/smithd"
                android:fontFamily="@font/uber_move"
                android:textColor="#000000"
                android:layout_marginTop="6dp">

            </TextView>

            <ImageView
                android:layout_width="24dp"
                android:layout_height="30dp"
                android:layout_marginRight="20dp"
                android:tint="#000000"
                android:layout_alignParentRight="true"
                android:id="@+id/neymarbarca"

                android:src="@drawable/ic_baseline_keyboard_arrow_right_24">

            </ImageView>

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:id="@+id/april"
            android:layout_below="@id/march"
            android:layout_marginTop="20dp">


            <TextView
                android:id="@+id/dimariad"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="6dp"
                android:layout_toRightOf="@id/carasco"
                android:text="Tablets"
                android:fontFamily="@font/uber_move"
                android:textColor="#000000">

            </TextView>

            <ImageView
                android:layout_width="24dp"
                android:layout_height="30dp"
                android:layout_marginRight="20dp"
                android:tint="#000000"
                android:layout_alignParentRight="true"
                android:id="@+id/neymarpsg"

                android:src="@drawable/ic_baseline_keyboard_arrow_right_24">

            </ImageView>

        </RelativeLayout>


    </RelativeLayout>



</RelativeLayout>